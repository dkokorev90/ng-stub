@import '~normalize-css/normalize'
@import './components/link/style'

@font-face
    font-family 'fontello'
    src url('assets/fonts/fontello.eot')
    src url('assets/fonts/fontello.eot#iefix') format('embedded-opentype')
    src url('assets/fonts/fontello.woff') format('woff')
    src url('assets/fonts/fontello.ttf') format('truetype')
    src url('assets/fonts/fontello.svg#fontello') format('svg')
    font-weight normal
    font-style normal

.page
    background #eee
